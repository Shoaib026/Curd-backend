{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
    }
    ],
    "env": {
      "NODE_ENV": "production",
      "CUSTOM_ENV_VARIABLE": "your_custom_value"
    }
  }
  